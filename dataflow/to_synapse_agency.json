{
	"name": "to_synapse_agency",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "pay_roll_agencymaster_ds",
						"type": "DatasetReference"
					},
					"name": "agencysourcecsv"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "payroll_agency_md_synapse_ds",
						"type": "DatasetReference"
					},
					"name": "agencysinksynapse"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> agencysourcecsv",
				"agencysourcecsv sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     staged: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          AgencyID,",
				"          AgencyName",
				"     )) ~> agencysinksynapse"
			]
		}
	}
}